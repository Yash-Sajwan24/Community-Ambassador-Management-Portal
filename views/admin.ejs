<div class="whole">
    <main class="col-md-9 <!--ms-sm-auto--> col-lg-10 px-md-4">
      <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
        <h1 class="h2">Admin Dashboard</h1>
      </div>



      <h2>Registrations</h2>
      <div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th scope="col">Name</th>
              <th scope="col">Institute</th>
              <th scope="col">Course</th>
              <th scope="col">Course Duration</th>
              <th scope="col">Email ID</th>
              <th scope="col">LinkedIN</th>
              <th scope="col">Resume</th>
            </tr>
          </thead>
          <tbody>
            <% for(let i = 0; i < detail.inputName.length ; i++){ %>
            <tr>
              <td><%= detail.inputName[i] %></td>
              <td><%= detail.institution[i] %></td>
              <td><%= detail.courseName[i] %></td>
              <td><%= detail.courseDuration[i] %></td>
              <td><%= detail.inputMail[i] %></td>
              <td><%= detail.social[i] %></td>
              <% let full = i + ".pdf"%>
              <td><a target="_blank" href=<%= full %>>Resume/CV.pdf</a></td>
              <!-- <td><%= detail.resume[i] %></td> -->
              <td id="selectedd">Selected</td>
              <form action="/admin" method="post" class="rem">
              <input type="hidden" name="selected" value=<%= detail.inputName[i] %>>
              <td><button id="select" type="submit">Select</button></td>
              <!-- <td id="rem1" + <%= i %>><button id="select" type="submit">Select</button></td> -->
            </form>
            <form action="/admin" method="post" class="rem">
              <input type="hidden" name="rejected">
              <td><button id="reject" type="submit">Reject</button></td>
              <!-- <td id="rem2"  + <%= i %>><button id="reject" type="submit">Reject</button></td> -->
            </form>
            </tr>
            <% } %>
          </tbody>
        </table>
      </div>
